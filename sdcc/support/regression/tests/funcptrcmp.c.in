/** Function pointer comparion tests - C requires pointers to different function to compare unequal even if their behavior is the same. Check this, to ensure any function merging optimizations keep this property.

    type: char, int, long
 */
#include <testfwk.h>

#ifdef __SDCC
#pragma std_sdcc99
#endif

#include <stdbool.h>

// Simple, short functions
{type} f1({type} i)
{
	return i + 1;
}

{type} g1({type} i)
{
	return i + 1;
}

// Longer functions
{type} f2({type} i, {type} j)
{
	return i * 5 - j * 3;
}

{type} g2({type} i, {type} j)
{
	return i * 5 - j * 3;
}

void
testFuncPtrCmp(void)
{
	ASSERT (&f1 != &g1);
	ASSERT (&f2 != &g2);
}

